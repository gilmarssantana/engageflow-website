# Se ainda não tem o repositório local
git clone https://github.com/gilmarssantana/engageflow-website.git
cd engageflow-website
# Baixar script automatizado
curl -o fix_engageflow_vercel.sh https://raw.githubusercontent.com/manus-ai/scripts/main/fix_engageflow_vercel.sh
# Dar permissão de execução
chmod +x fix_engageflow_vercel.sh

# Executar script
./fix_engageflow_vercel.sh
